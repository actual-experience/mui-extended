{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "target": "ES5",
    "module": "esnext",
    "rootDir": "./src",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "preserve",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "next-env.d.ts",
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
