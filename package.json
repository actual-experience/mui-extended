{
  "name": "@solib/ui-components",
  "version": "0.0.3",
  "description": "Basic UI Components for Sodaru Projects",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "npx rimraf dist",
    "prettier": "npx prettier --check --ignore-unknown ./**/*",
    "eslint": "npx eslint ./ --no-error-on-unmatched-pattern",
    "tsc": "npx tsc --project tsconfig.build.json",
    "copyAssets": "cp src/logo.png dist/logo.png",
    "build": "npm run prettier && npm run eslint && npm run clean && npm run tsc && npm run copyAssets",
    "pretest": "npm run build",
    "test": "echo 'no tests'",
    "prepack": "npm run test",
    "postversion": "git push --follow-tags",
    "dev": "next dev",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/sodaru/solib/ui-components.git"
  },
  "author": "Raghavendra K R <raghavendra@sodaru.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://gitlab.com/sodaru/solib/ui-components/issues"
  },
  "homepage": "https://gitlab.com/sodaru/solib/ui-components#readme",
  "devDependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.5.0",
    "@mui/material": "^5.4.4",
    "@next/bundle-analyzer": "^12.1.0",
    "@sodev/eslint-config": "^1.1.0",
    "@sodev/prettier-config": "^1.1.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "eslint-config-next": "^12.1.0",
    "next": "^12.1.0",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2"
  },
  "prettier": "@sodev/prettier-config",
  "eslintConfig": {
    "extends": [
      "@sodev/eslint-config",
      "next"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.5.0",
    "@mui/material": "^5.4.4",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "tslib": "^1.14.1"
  }
}
