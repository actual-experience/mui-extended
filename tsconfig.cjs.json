{
  "compilerOptions": {
    "outDir": "dist/cjs",
    "rootDir": "./src",
    "target": "ES5",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "importHelpers": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": ["src"]
}
